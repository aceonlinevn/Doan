<%
ConnectionPool cp = (ConnectionPool) application.getAttribute("cpool");
ProductControl pc = new ProductControl(cp);
if(cp == null){
  application.setAttribute("cpool",pc.getConnectionPool());
}
//Khai bảo object
ProductObject similar = new ProductObject();
//Khởi tạo điểu kiện

//Khởi tạo cấu trúc
String viewProductForCondition = pc.viewProducts(similar, 0 , (byte) 5);

//Dua cau truc vao phien lam viec
session.setAttribute("viewProductForCondition", viewProductForCondition);

%>