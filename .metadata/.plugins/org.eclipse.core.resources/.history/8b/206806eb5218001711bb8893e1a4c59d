/**
 * javascript for common
 */

function confirmDel(url){
	var message = "Bạn có chắc chắn xóa không ?";
	if(window.confirm(message)){
		window.location.href = url;
	}
	else{
		return false;
	}
	
}
function saveProduct(fn){
	fn.method = "POST";
	fn.action = "/WebBanHang/category/ae";
	fn.submit();
}