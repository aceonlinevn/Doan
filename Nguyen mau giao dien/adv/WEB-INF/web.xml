<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd" version="2.4">
  <display-name>adv</display-name>
  <servlet>
    <servlet-name>userlogin</servlet-name>
    <servlet-class>jsoft.ads.user.UserLogin</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>userlogin</servlet-name>
    <url-pattern>/user/login</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>view</servlet-name>
    <servlet-class>jsoft.ads.main.View</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>view</servlet-name>
    <url-pattern>/view</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>userlogout</servlet-name>
    <servlet-class>jsoft.ads.user.UserLogout</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>userlogout</servlet-name>
    <url-pattern>/user/logout</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>userview</servlet-name>
    <servlet-class>jsoft.ads.user.UserView</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>userview</servlet-name>
    <url-pattern>/user/view</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>userae</servlet-name>
    <servlet-class>jsoft.ads.user.UserAE</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>userae</servlet-name>
    <url-pattern>/user/ae</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>userdel</servlet-name>
    <servlet-class>jsoft.ads.user.UserDel</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>userdel</servlet-name>
    <url-pattern>/user/del</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>header</servlet-name>
    <servlet-class>jsoft.ads.main.header</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>header</servlet-name>
    <url-pattern>/header</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>menu</servlet-name>
    <servlet-class>jsoft.ads.main.menu</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>menu</servlet-name>
    <url-pattern>/menu</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>footer</servlet-name>
    <servlet-class>jsoft.ads.main.footer</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>footer</servlet-name>
    <url-pattern>/footer</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>SectionView</servlet-name>
    <servlet-class>jsoft.ads.article.section.SectionView</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SectionView</servlet-name>
    <url-pattern>/section/view</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>sectionae</servlet-name>
    <servlet-class>jsoft.ads.article.section.SectionAE</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>sectionae</servlet-name>
    <url-pattern>/section/ae</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>CategoryView</servlet-name>
    <servlet-class>jsoft.ads.article.category.CategoryView</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CategoryView</servlet-name>
    <url-pattern>/category/view</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>ArticleView</servlet-name>
    <servlet-class>jsoft.ads.article.ArticleView</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ArticleView</servlet-name>
    <url-pattern>/article/view</url-pattern>
  </servlet-mapping>
  <servlet>
    <description>Added by JBuilder to compile JSPs with debug info</description>
    <servlet-name>debugjsp</servlet-name>
    <servlet-class>org.apache.jasper.servlet.JspServlet</servlet-class>
    <init-param>
      <param-name>classdebuginfo</param-name>
      <param-value>true</param-value>
    </init-param>
    <load-on-startup>3</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>debugjsp</servlet-name>
    <url-pattern>*.jsp</url-pattern>
  </servlet-mapping>
</web-app>

